<div id="lbContent" class="distribution-plugin-popin">
  <h1><%= _("Session editing") %></h1>

  <div>
    <% if session.valid? %>
      <%= _('Session saved.') %>
    <% else %>
      <% session.errors.each_full do |msg| %>
        <div><%= msg %></div>
      <% end %>
    <% end %>
  </div>

  <div>
     <%= link_to_function _('close'), 'lightbox.prototype.deactivate()' %>
  </div>

</div>
