<% if element.class.name == "Product" %>

  <h6 class="element-title">
  <%= link_to(element.category_name) + " " + link_to(element.name) %>
  </h6>

  <%= link_to_remote "<button class='action-button exchange-add'>+</button>",
  :url => {:action => "add_element", :element_id => element.id,
  :element_type => "Product", :enterprise => enterprise,
  :proposal_id => proposal_id, :enterprise_id => element.enterprise_id} %>

<% elsif element.class.name == "CmsLearningPlugin::Learning" %>

  <h6 class="element-title">
  <%= link_to(element.name) %>
  </h6>

  <%= link_to_remote "<button class='action-button exchange-add'>+</button>",
  :url => {:action => "add_element", :element_id => element.id,
  :enterprise_id => element.profile_id, :element_type => "CmsLearningPlugin::Learning", :proposal_id => proposal_id, :enterprise => enterprise} %>


<% end %>



