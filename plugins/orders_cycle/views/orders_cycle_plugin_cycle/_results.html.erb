<% if @closed_cycles.blank? %>
  <div class="subtitle"><%= t('views.cycle._results.no_cycles_to_show') %></div>
<% else %>
  <% @closed_cycles.each do |cycle| %>
    <div id="cycle-list-item-<%= cycle.id %>" class="cycle-list-item">
      <%= render 'header', :cycle => cycle, :timeline => true, :actions => false, :listing => true %>
    </div>
  <% end %>
<% end %>
