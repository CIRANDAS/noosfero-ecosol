<% extend ManageProductsHelper %>

<h1><%= @page_tab.title %></h1>
<% if (!@page_tab.subtitle.empty?) %>
  <div id="page-tab-subtitle"><%= @page_tab.subtitle %></div>
<% end %>

<%= button :back, t('fb_app_plugin.views.page_tab.back_to_catalog'), params.except(:product_id), target: '' %>

<%= render file: "#{Rails.root}/app/views/manage_products/show.html.erb", :locals=>{:show_supplier => true} %>

<%= button :back, t('fb_app_plugin.views.page_tab.back_to_catalog'), params.except(:product_id), target: '' %>

<%= render 'load' %>
