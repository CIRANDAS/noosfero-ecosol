<nav class="navbar navbar-inverse" id="">
  <ul class="nav navbar-nav">
    <% links.each do |link| %>
      <li>
        <%= block.sanitize_link(link_to link[:name], block.expand_address(link[:address]), target: link[:target], class: "#{'icon-' + link[:icon] if link[:icon]}", title: link[:title]) %>
      </li>
    <% end %>
  </ul>
</nav>
