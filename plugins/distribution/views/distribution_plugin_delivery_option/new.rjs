if params[:new_method]
  page << "jQuery('.distribution-plugin-new-delivery-method form input').val('')"
  page << "jQuery('.distribution-plugin-new-delivery-method').hide()"
else
  page.remove 'distribution-plugin-delivery-method-as-option-'+@delivery_method.id.to_s
end

page.insert_html :bottom, 'distribution-plugin-session-delivery-options', :partial => 'show'
page << 'lightbox.prototype.deactivate()'

