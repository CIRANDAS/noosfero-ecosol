<h1> <%= t('orders_plugin.lib.plugin.panel_button') %> </h1>

<div class='ui-tabs'>
  <ul>
    <li class='tab'><a href='#purchases'><%= t('orders_plugin.terms.purchase.plural') %></a></li>
    <li class='tab'><a href='#sales'><%= t('orders_plugin.terms.sale.plural') %></a></li>
  </ul>

  <div id='purchases'>
    <%= render 'orders_plugin_admin/table', :orders => profile.purchases %>
  </div>

  <div id='sales'>
    <%= render 'orders_plugin_admin/table', :orders => profile.sales %>
  </div>
</div>

