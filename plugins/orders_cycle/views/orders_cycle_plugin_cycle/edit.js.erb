<% if params[:commit] %>
  <% if @success and @open %>
    window.location = <%= url_for(:action => :edit, :id => @cycle.id).to_json %>
  <% else %>
    orders_cycle.colorbox({html: '<%= escape_javascript render('edit_popin', :cycle => @cycle) %>'})
  <% end %>
<% else %>
  jQuery("#cycle-product-lines").replaceWith('<%= escape_javascript render('product_lines') %>')
<% end %>
