<h3><%= _('ERROR') %></h3>
<p>
  <%= "State when error ocurred: #{@project.state}" %>
  <br/>
  <% error = @project.kalibro_error %>
  <%= error.message %>
<ul>
  <% error.stack_trace.each do |trace| %>
    <li><%= "#{trace.declaring_class}.#{trace.method_name}(#{trace.file_name}:#{trace.line_number})" %></li>
  <% end %>
</ul>
</p>
