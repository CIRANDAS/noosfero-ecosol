<div id="cycle-admin-page" class='admin-page'>

  <div id="cycle-<%= @cycle.id %>" class="cycle">
    <%= render 'header', :cycle => @cycle, :timeline => true, :actions => true, :listing => false %>

    <% view = if params[:view] then params[:view] else @cycle.status end %>
    <div>
      <%= render view %>
    </div>
  </div>
  <%= render 'report_modal', :cycle => @cycle %>
  <%= render 'add_payment_modal' %>
</div>
