<%
  urls = [
    [t('distribution_plugin.views.layouts._admin_sidebar.orders_cycles'), {:controller => :distribution_plugin_session, :action => :index, :profile => profile.identifier}, false],
    #[t('distribution_plugin.views.layouts._admin_sidebar.parcels'), {:controller => :distribution_plugin_parcel, :action => :index, :profile => profile.identifier}, false],
    [t('distribution_plugin.views.layouts._admin_sidebar.products'), {:controller => :distribution_plugin_product, :action => :index, :profile => profile.identifier}, false],
    [t('distribution_plugin.views.layouts._admin_sidebar.suppliers'), {:controller => :distribution_plugin_supplier, :action => :index, :profile => profile.identifier}, false],
    #[t('distribution_plugin.views.layouts._admin_sidebar.members'), {:controller => :distribution_plugin_collective, :action => :members, :profile => profile.identifier}, true],
  ]
%>
<% urls.each do |name, url, exact| %>
  <%= link_to name, url, :class => 'menu-entry' +
    ((params[:controller] == url[:controller].to_s and (!exact or params[:action] == url[:action].to_s)) ? ' menu-selected' : '') %>
<% end %>
