<% if form_errors.errors.any? %>
  <div>
    <strong><%= t('distribution_plugin.views.product._form_errors.errors_found') %></strong>
    <ul>
      <% form_errors.errors.each do |attr,message| %>
        <li><%= "#{_(attr.humanize)} #{message}" %></li>
      <% end %>
    </ul>
  </div>
<% end %>

