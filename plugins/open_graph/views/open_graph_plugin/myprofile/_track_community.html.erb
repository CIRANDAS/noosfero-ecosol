<div class="panel-heading">
  <%= button_to_function 'menu-ctrl-panel', t('open_graph_plugin.views.track.config.activity.configure'), 'open_graph.track.config.open(this)',
href: "#track-#{track}", class: 'community-config', option: 'success', size: 'xs', 'data-toggle' => 'collapse', 'aria-controls' => 'collapseTwo' %>
  <a href="#track-<%=track%>" data-toggle="collapse" aria-controls="collapseTwo" onclick='open_graph.track.config.toggle(this, event)', class='community-label'>
    <%= render 'heading', f: f, track: track, context: context, klass: klass %>
  </a>
</div>

<div id="track-<%=track%>" class="panel-body collapse">
  <%= render 'track_objects', f: f, track: track, objects: klass.objects, klass: klass, context: context %>
  <%= render 'track_profiles', f: f, track: track, context: context, klass: klass %>

</div>
