<div class="distribution-plugin-popin">
  <h1><%= _("Cycle editing") %></h1>

  <div>
    <% if session.valid? %>
      <%= _('Cycle saved.') %>
    <% else %>
      <% session.errors.each_full do |msg| %>
        <div><%= msg %></div>
      <% end %>
    <% end %>
  </div>

  <div>
    <%= colorbox_close_button _('Close') %>
  </div>

</div>
