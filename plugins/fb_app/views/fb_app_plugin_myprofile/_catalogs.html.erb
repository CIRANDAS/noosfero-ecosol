<h2>
  <%= t('fb_app_plugin.views.myprofile.catalogs.heading') %>

  <% profile.fb_app_page_tabs.each do |page_tab| %>
    <%= render 'catalog', page_tab: page_tab %>
  <% end %>

  <div id="new-catalog">
    <%= render 'catalog', page_tab: profile.fb_app_page_tabs.build(profile: profile) %>
  </div>
</h2>

